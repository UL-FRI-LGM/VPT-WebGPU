{"AboutText":"<!-- #link /css/AboutText -->\r\n\r\n<div class=\"instantiate about\">\r\n    <h3>The Volumetric Path Tracing (VPT) Framework</h3>\r\n\r\n    <div class=\"center\">\r\n        <img src=\"images/favicon-32x32.png\">\r\n    </div>\r\n\r\n    <p>\r\n        Laboratory for Computer Graphics and Multimedia<br/>\r\n        University of Ljubljana<br/>\r\n        Faculty of Computer and Information Science\r\n    </p>\r\n\r\n    <p>\r\n        For more information visit the\r\n        <a href=\"http://lgm.fri.uni-lj.si/portfolio-view/volumetric-path-tracing-framework/\">portfolio page</a>\r\n        or the\r\n        <a href=\"https://github.com/terier/vpt\">github page</a>.\r\n    </p>\r\n\r\n    <p>© 2019 Žiga Lesar</p>\r\n</div>\n","TransferFunctionWidget":"<!-- #link /css/TransferFunctionWidget -->\r\n\r\n<div class=\"instantiate transfer-function-widget\">\r\n  <div class=\"widget\">\r\n    <canvas></canvas>\r\n  </div>\r\n  <div class=\"controls\">\r\n    <input type=\"button\" class=\"button\" value=\"Add bump\" name=\"add-bump\">\r\n    <input type=\"button\" class=\"button\" value=\"Remove selected bump\" name=\"remove-selected-bump\">\r\n    <input type=\"button\" class=\"button\" value=\"Remove all bumps\" name=\"remove-all-bumps\">\r\n    <input type=\"button\" class=\"button\" value=\"Load\" name=\"load\">\r\n    <input type=\"button\" class=\"button\" value=\"Save\" name=\"save\">\r\n    <div class=\"panel\">\r\n      <label>Color</label>\r\n      <input name=\"color\" type=\"color\">\r\n    </div>\r\n    <div>\r\n      <label>Alpha</label>\r\n      <input name=\"alpha\" type=\"number\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\">\r\n    </div>\r\n  </div>\r\n</div>\n","TransferFunctionWidgetBump":"<!-- #link /css/TransferFunctionWidget -->\r\n\r\n<div class=\"instantiate bump\">\r\n  <div class=\"bump-handle\"></div>\r\n</div>\n","ui":{"Accordion":"<!-- #link /css/ui/Accordion -->\r\n\r\n<div class=\"instantiate accordion\">\r\n    <div class=\"handle\" data-bind=\"handle\"></div>\r\n    <div class=\"container\" data-bind=\"container\"></div>\r\n</div>\n","Button":"<!-- #link /css/ui/Button -->\r\n\r\n<div class=\"instantiate button\">\r\n    <input type=\"button\" data-bind=\"input\">\r\n</div>\n","Checkbox":"<!-- #link /css/ui/Checkbox -->\r\n\r\n<div class=\"instantiate checkbox\">\r\n    <div class=\"handle\" data-bind=\"handle\"></div>\r\n</div>\n","ColorChooser":"<!-- #link /css/ui/ColorChooser -->\r\n\r\n<div class=\"instantiate color-chooser\">\r\n    <div class=\"color\" data-bind=\"color\"></div>\r\n    <input type=\"color\" data-bind=\"input\">\r\n</div>\n","Dropdown":"<!-- #link /css/ui/Dropdown -->\r\n\r\n<div class=\"instantiate dropdown\">\r\n    <select data-bind=\"input\">\r\n        <option value=\"\" disabled selected hidden>Select...</option>\r\n    </select>\r\n</div>\n","Field":"<!-- #link /css/ui/Field -->\r\n\r\n<div class=\"instantiate field\">\r\n    <label data-bind=\"label\"></label>\r\n    <div class=\"container\" data-bind=\"container\"></div>\r\n</div>\n","FileChooser":"<!-- #link /css/ui/FileChooser -->\r\n\r\n<div class=\"instantiate file-chooser\">\r\n    <div class=\"label\" data-bind=\"label\"></div>\r\n    <input type=\"file\" data-bind=\"input\">\r\n</div>\n","Panel":"<div class=\"instantiate panel\">\r\n</div>\n","ProgressBar":"<!-- #link /css/ui/ProgressBar -->\r\n\r\n<div class=\"instantiate progress-bar\">\r\n    <div class=\"progress\" data-bind=\"progress\"></div>\r\n    <div class=\"label\" data-bind=\"label\"></div>\r\n</div>\n","Radio":"<!-- #link /css/ui/Radio -->\r\n\r\n<div class=\"instantiate radio\">\r\n</div>\n","RadioOption":"<!-- #link /css/ui/Radio -->\r\n\r\n<div class=\"instantiate radio-option\">\r\n    <input type=\"radio\" data-bind=\"input\">\r\n    <label data-bind=\"label\"></label>\r\n</div>\n","Sidebar":"<!-- #link /css/ui/Sidebar -->\r\n\r\n<div class=\"instantiate sidebar\">\r\n    <div class=\"handle\" data-bind=\"handle\"></div>\r\n    <div class=\"container\" data-bind=\"container\"></div>\r\n</div>\n","Slider":"<!-- #link /css/ui/Slider -->\r\n\r\n<div class=\"instantiate slider\">\r\n    <div class=\"container\" data-bind=\"container\">\r\n        <div class=\"track\"></div>\r\n        <div class=\"button\" data-bind=\"button\"></div>\r\n    </div>\r\n</div>\n","Spacer":"<div class=\"instantiate spacer\"></div>\n","Spinner":"<!-- #link /css/ui/Spinner -->\r\n\r\n<div class=\"instantiate spinner\">\r\n    <input type=\"number\" data-bind=\"input\">\r\n</div>\n","StatusBar":"<!-- #link /css/ui/StatusBar -->\r\n\r\n<div class=\"instantiate status-bar\">\r\n</div>\n","StatusBarLog":"<!-- #link /css/ui/StatusBar -->\r\n\r\n<div class=\"instantiate log\">\r\n    <div class=\"timestamp\" data-bind=\"timestamp\"></div>\r\n    <div class=\"content\" data-bind=\"content\"></div>\r\n</div>\n","Tabs":"<!-- #link /css/ui/Tabs -->\r\n\r\n<div class=\"instantiate tabs\">\r\n    <div class=\"headers\" data-bind=\"headers\">\r\n        <div class=\"filler\"></div>\r\n    </div>\r\n    <div class=\"container\" data-bind=\"container\"></div>\r\n</div>\n","Textbox":"<!-- #link /css/ui/Textbox -->\r\n\r\n<div class=\"instantiate textbox\">\r\n    <input type=\"text\" data-bind=\"input\">\r\n</div>\n","VectorSpinner":"<!-- #link /css/ui/VectorSpinner -->\r\n\r\n<div class=\"instantiate vector\">\r\n    <div class=\"vector-component\" data-bind=\"vectorX\"></div>\r\n    <div class=\"vector-component\" data-bind=\"vectorY\"></div>\r\n    <div class=\"vector-component\" data-bind=\"vectorZ\"></div>\r\n</div>\n"}}